<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:DotnetRss.Maui"
             xmlns:converters="clr-namespace:DotnetRss.Maui.Converters"
             x:Class="DotnetRss.Maui.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles.xaml" />
                <ResourceDictionary Source="DataTemplates.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <converters:IntEnumConverter x:Key="IntEnumConverter"/>
            <converters:UrlImageConverter x:Key="UrlImageConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
